{
    "cloudSql": {
        "ConnectionString": "Host=integrations_postgres;Port=5432;Username=postgres;Password=postgres;Database=integrations"
    },
    "DataProtectionCertPath": {
        "Path": "./Certs/integrations-protection-cert.pfx"
    },
    "rabbitMQ": {
        "host": "rabbitmq",
        "port": "5672",
        "virtualHost": "/",
        "username": "integrationsUser",
        "password": "integrationsPass",
        "namespace": "integrations",
        "retries": 3,
        "retryInterval": 3
    },
    "Serilog": {
        "Using": ["Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        },
        "Enrich": ["FromLogContext"],
        "WriteTo": [
            {
                "Name": "DatadogLogs",
                "Args": {
                    "apiKey": "",
                    "service": "Integrations",
                    "source": "Serilog",
                    "host": "Nick's Laptop"
                }
            }
        ]
    }
}
